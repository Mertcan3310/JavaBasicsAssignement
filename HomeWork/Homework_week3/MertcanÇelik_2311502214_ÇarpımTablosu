import java.util.*;


public class Main {

    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);

        System.out.println("Bir sayi giriniz : \n");
        int sayi = input.nextInt();
        int cevap=0;
        for(int i=0;i<11;i++){
            cevap= sayi*i;
            System.out.println(sayi+"x"+i+"="+cevap);
        }
    }
}
